{
  "name": "restaurants-menu-api",
  "version": "0.0.1",
  "author": "AIS",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "npm run build && node build/index.js",
    "start:dev": "nodemon",
    "clean": "rimraf build",
    "build": "rimraf ./build && tsc",
    "lint": "eslint --ignore-path .gitignore src",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.8.0",
    "aws-sdk": "^2.702.0",
    "babel": "^6.23.0",
    "body-parser": "^1.19.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "elasticsearch": "^16.7.1",
    "express": "~4.17.1",
    "glob": "^7.1.6",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "mysql": "2.18.1",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.16.1",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      [
        "es2015"
      ],
      [
        "stage-0"
      ]
    ]
  },
  "eslintConfig": {
    "plugins": [
      "import"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 1
    }
  }
}
